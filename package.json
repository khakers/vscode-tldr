{
  "name": "vscode-tldr",
  "displayName": "tl;dr pages",
  "description": "Hover for commands using simplified and community-driven man pages.",
  "version": "2.0.0-beta.1",
  "publisher": "khakers",
  "license": "MIT",
  "engines": {
    "vscode": "^1.33.0"
  },
  "icon": "images/tldr-books.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/khakers/vscode-tldr.git"
  },
  "homepage": "https://github.com/khakers/vscode-tldr/blob/master/README.md",
  "galleryBanner": {
    "color": "#313131",
    "theme": "dark"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "keywords": [
    "tldr",
    "man",
    "shell",
    "cli",
    "bash"
  ],
  "activationEvents": [
    "onLanguage:dockerfile",
    "onLanguage:makefile",
    "onLanguage:powershell",
    "onLanguage:shellscript",
    "onLanguage:bat"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "node-fetch": "^3.2.5"
  },
  "devDependencies": {
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.67.0",
    "tslint": "^5.20.1",
    "typescript": "^4.7.2"
  }
}
